 }
<div style="display:flex;">
    @Html.EJS().DateTimePicker(Model[0]).Placeholder("Select date and time").Render()
    @Html.EJS().DropDownList(Model[1]).DataSource(timeZones).Placeholder("Select time zone").Render()
</div>
